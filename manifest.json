{
  "manifest_version": 3,
  "name": "Canvas Hello Banner",
  "version": "1.0",
  "description": "Adds a simple banner above the Canvas dashboard.",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["https://*.instructure.com/*"],
      "js": ["content.js"]
    }
  ]
}

